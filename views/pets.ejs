<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Pets</title>
  </head>
  <body>
    <h2>Pets Database</h2>
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Name</th>
          <th>Species</th>
          <th>Gender</th>
        </tr>
      </thead>
      <tbody>
        <% for (let pet of pets) { %>
          <tr>
            <td><%= pet.id %></td>
            <td><%= pet.name %></td>
            <td><%= pet.type %></td>
            <td><%= pet.gender %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </body>
</html>
